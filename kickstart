echo "=][=  Installing Some basic pacakges"
echo ""
sudo apt-get -qq install ansible aptitude git

echo ""
echo "=][=  Cloning the repo"
echo ""
git clone https://github.com/thecoshman/zipper.git

echo ""
echo "=][=  Running boot_strap playbook"
cd zipper/ansible
sudo ansible-playbook boot_strap.yml -i inventory
